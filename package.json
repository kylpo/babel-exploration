{
  "private": true,
  "scripts": {
    "build": "npm run build:constelation && npm run build:emotion && npm run build:constant && npm run build:inline",
    "build:constelation": "babel 0-start.js --out-file 1-after-constelation.js --plugins=constelation-babel",
    "build:emotion": "babel 1-after-constelation.js --out-file 2-after-emotion.js --plugins=emotion/babel",
    "build:constant": "babel 2-after-emotion.js --out-file 3a-after-constant.js --plugins=transform-react-constant-elements",
    "build:inline": "babel 2-after-emotion.js --out-file 3b-after-inline.js --plugins=transform-react-inline-elements"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "constelation-babel": "^15.0.2"
  },
  "dependencies": {
    "emotion": "^6.0.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  }
}
