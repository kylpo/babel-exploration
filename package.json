{
  "private": true,
  "scripts": {
    "build": "npm run build:primitives && npm run build:linaria && npm run build:styled-jsx && npm run build:constant && npm run build:inline && npm run build:prod && npm run build:jsx",
    "build:primitives": "babel 0-start.js --out-file 1-after-primitives.js --plugins=module:chronstruct-primitives",
    "build:linaria": "BABEL_ENV='extract' babel 1-after-primitives.js --out-file 2-after-linaria.js",
    "build:styled-jsx": "BABEL_ENV='styledJsx' babel 1-after-primitives.js --out-file 2-after-styledJsx.js",
    "build:constant": "babel 2-after-linaria.js --out-file 3a-after-constant.js --plugins=@babel/transform-react-constant-elements",
    "build:inline": "babel 2-after-linaria.js --out-file 3b-after_inline.js --plugins=@babel/transform-react-inline-elements",
    "build:prod": "babel 2-after-linaria.js --out-file 3-after-prod.js --plugins=@babel/transform-react-constant-elements,@babel/transform-react-inline-elements",
    "build:jsx": "babel 3-after-prod.js --out-file 4-after-jsx.js --plugins=@babel/transform-react-jsx"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babili-webpack-plugin": "^0.1.2"
  },
  "dependencies": {
    "linaria": "^1.3.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "styled-jsx": "^3.2.5"
  }
}
